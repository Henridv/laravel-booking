    "eslint-plugin-import": "^2.22.1",
    "jquery": "^3.5.1",
    "laravel-mix": "^5.0.1",
    "lodash": "^4.17.20",
    "popper.js": "^1.16.1",
    "resolve-url-loader": "^3.1.2",
    "sass": "^1.32.5",
    "sass-loader": "^8.0.0",
    "vue-template-compiler": "^2.6.12"
  },
